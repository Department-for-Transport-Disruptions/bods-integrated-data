{
    "name": "@bods-integrated-data/aggregate-siri",
    "version": "0.1.0",
    "description": "This function is to regularly query the AVL database to get the most recent vehicle location for each unique vehicle in the database and aggregate it into a single SIRI-VM file.",
    "scripts": {
        "test": "vitest run",
        "build:ci": "rm -rf ./dist && tsc && node ./esbuild.mjs && cd ./dist && zip -rq ./aggregate-siri.zip .",
        "build:local": "pnpm run build:ci && mkdir -p ../dist && cp ./dist/aggregate-siri.zip ../dist/aggregate-siri.zip"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "module",
    "dependencies": {
        "kysely": "^0.27.2",
        "lambda-log": "^3.1.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.110",
        "@types/lambda-log": "^2.2.1",
        "@types/node": "^20.11.19"
    }
}