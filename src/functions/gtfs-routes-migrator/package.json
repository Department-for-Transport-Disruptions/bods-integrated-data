    "dependencies": {
        "@bods-integrated-data/shared": "workspace:^",
        "axios": "^1.9.0",
        "kysely": "0.27.3",
        "papaparse": "^5.4.1",
        "unzipper": "^0.10.14",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.110",
        "@types/node": "^20.11.19",
        "@types/papaparse": "^5.3.14",
        "@types/unzipper": "^0.10.9"
    }
}
