    "dependencies": {
        "@aws-sdk/client-secrets-manager": "3.362.0",
        "@baselime/lambda-logger": "^0.2.2",
        "@bods-integrated-data/shared": "workspace:^",
        "basic-ftp": "^5.0.5"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.110",
        "@types/node": "^20.11.19"
    }
}
