    "dependencies": {
        "@bods-integrated-data/shared": "workspace:^",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.110",
        "@types/node": "^20.11.19"
    }
}
